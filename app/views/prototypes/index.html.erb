<main class="main">
  <div class="inner">
    <div class="greeting">
      <% if user_signed_in? %>
        <%="こんにちは、"%>
        <%=link_to current_user.name + "さん",user_path(current_user), class: :greeting__link%>
      
      <% end %>
    </div>
    <div class="card__wrapper">
      <% if user_signed_in? %>
       <%= render partial: "prototypes/prototype",collection:@prototypes %>
      <% end %>
    </div>
  </div>
</main>
